{
  "env": {
    "browser": true,
    "es2021": true
  },
  
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "prettier"],
  "plugins": ["prettier", "import"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error",{ "endOfLine": "auto" }],
    "import/prefer-default-export": "off",
    "consistent-return":"off"
  }
}
